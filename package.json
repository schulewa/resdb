{
  "name": "resdb",
  "version": "0.0.1-SNAPSHOT",
  "scripts": {
    "prebuilt": "npm run lint",
    "postinstall": "webdriver-manager update",
    "clean": "rimraf dist && rimraf app/*.js* && rimraf app/*.map",
    "builddev": "npm run clean && webpack --config ./config/webpack.dev.js --progress --profile --bail",
    "ng": "ng",
    "start": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress --port 8010",
    "build": "npm run clean && webpack --config ./config/webpack.prod.js --progress --bail",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "e2e": "protractor protactor.config.js --kill-others --success first",
    "lint": "tslint --type-check ./src/tsconfig.json ./src/**/*.ts -t verbose",
    "server": "http-server -s dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "~7.2.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "ag-grid-angular": "^19.1.2",
    "ag-grid-community": "^19.1.4",
    "bootstrap": "^4.2.1",
    "core-js": "^2.5.4",
    "jquery": "^3.3.1",
    "ngx-permissions": "^6.0.5",
    "popper.js": "^1.14.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
